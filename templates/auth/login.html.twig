<h1>Login</h1>

{% if error is defined %}
{{ dump(error) }}
{% endif %}

<form method="POST" action="/login">
    <label>Username:
        <input type="text" name="username" required>
    </label><br>
    <label>Password:
        <input type="password" name="password" required>
    </label><br>
    <button type="submit">Login</button>
</form>
